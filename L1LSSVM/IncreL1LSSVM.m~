function [outputArg1,outputArg2] = IncreL1LSSVM(X,y,C,w,lr)
% For more information, read readMe.md
    e = ones(size(X,1),1);
    Y = diag(y);
    max_iter=100;
    objVal = norm(w,1)+C*e'*norm(e-Y*X*w);
    
    epsion = 1e-7;
    Diff = inf;
    iter=1;
    while Diff>1e-4 || iter<max_iter
        outlier = e-Y*X*w;
        outlier(find(outlier==0))=epsion;
        w(find(w==0))=epsion;
        derivation = sign(w)+C*(e'*diag(e-))
        
        
        
        
        
        
        iter = iter+1;
    end
    


end

